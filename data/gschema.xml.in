<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="openemail">
	<schema id="@APP_ID@" path="@PREFIX@/">
		<key name="address" type="s">
			<default>""</default>
		</key>
		<key name="sync-interval" type="u">
			<default>900</default>
		</key>
		<key name="trashed-message-ids" type="as">
			<default>[]</default>
		</key>
	</schema>
	<schema id="@APP_ID@.State" path="@PREFIX@/State/">
		<key name="width" type="i">
			<default>1080</default>
		</key>
		<key name="height" type="i">
			<default>600</default>
		</key>
		<key name="show-sidebar" type="b">
			<default>true</default>
		</key>
	</schema>
</schemalist>
